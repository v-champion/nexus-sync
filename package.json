{
  "name": "nexus-sync",
  "displayName": "Roblox Nexus Sync",
  "description": "Allows you to sync your Roblox Studio output with Visual Studio Code",
  "publisher": "v-champion",
  "author": "v-champion",
  "license": "MIT",
  "icon": "icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./src/extension.js",
  "languages": [],
  "contributes": {
    "commands": [],
    "menus": {
      "editor/context": []
    },
    "configuration": {
      "title": "Roblox Nexus Sync",
      "properties": {
        "nexus-sync.plugin.port": {
          "type": "number",
          "default": 50583,
          "description": "Port number to connect to the Studio Plugin",
          "scope": "window"
        },
        "nexus-toolset.plugin.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enables the Studio Plugin to connect to the extension",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {}
}